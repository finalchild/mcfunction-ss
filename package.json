{
    "name": "mcfunction-ss",
    "displayName": "mcfunction-ss",
    "description": "Language support for mcfunction",
    "version": "0.2.5",
    "publisher": "intsuc",
    "engines": {
        "vscode": "^1.18.0"
    },
    "license": "MIT",
    "categories": [
        "Languages"
    ],
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/intsuc/mcfunction-ss"
    },
    "keywords": [
        "mcfunction"
    ],
    "activationEvents": [
        "onLanguage:mcfunction"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "mcfunction",
                "aliases": [
                    "mcfunction"
                ],
                "extensions": [
                    ".mcfunction"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mcfunction",
                "scopeName": "source.mcfunction",
                "path": "./syntaxes/mcfunction.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.51",
        "glob": "^7.1.2",
        "ts-pegjs": "^0.1.13",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    }
}
