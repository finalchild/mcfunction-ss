{
    "name": "mcfunction-ss",
    "displayName": "mcfunction-ss",
    "description": "",
    "version": "0.0.1",
    "publisher": "intsuc",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:mcfunction"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "mcfunction",
                "aliases": [
                    "mcfunction"
                ],
                "extensions": [
                    ".mcfunction"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [{
            "language": "mcfunction",
            "scopeName": "source.mcfunction",
            "path": "./syntaxes/mcfunction.json"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}